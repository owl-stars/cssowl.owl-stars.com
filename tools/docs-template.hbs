%h3#{{mixinName}} {{mixinName}} <small>({{join mixinParams ", "}})</small>

:markdown
  {{{indent description}}}

= preserve do
  -code("sass") do
    :plain
      /*
       {{#each mixin.parameters}}* @param ${{@key}} {{this}}
       {{/each}}*/
      {{{indent mixin.sass 3}}}

.bs-docs-example

  .content
    {{{indent examples.html 2}}}

    :sass
      @import "bower_components/cssowl/lib/sass/cssowl"
      {{{indent examples.sass 3}}}

  %ul.nav.nav-tabs
    %li.active.htmltab
      = link_to 'HTML', '#example-{{{basename}}}-html'
    %li
      = link_to 'LESS', '#example-{{{basename}}}-less'
    %li
      = link_to 'SASS', '#example-{{{basename}}}-sass'
    %li
      = link_to 'SCSS', '#example-{{{basename}}}-scss'
    %li
      = link_to 'Stylus', '#example-{{{basename}}}-styl'

  .tab-content
    #example-{{{basename}}}-html.tab-pane.active
      = preserve do
        -code("html") do
          {{{indent examples.html 5}}}
    #example-{{{basename}}}-less.tab-pane
      = preserve do
        -code("less") do
          :plain
            {{{indent examples.less 6}}}
    #example-{{{basename}}}-sass.tab-pane
      = preserve do
        -code("sass") do
          :plain
            {{{indent examples.sass 6}}}
    #example-{{{basename}}}-scss.tab-pane
      = preserve do
        -code("scss") do
          :plain
            {{{indent examples.scss 6}}}
    #example-{{{basename}}}-styl.tab-pane
      = preserve do
        -code("styl") do
          :plain
            {{{indent examples.styl 6}}}
